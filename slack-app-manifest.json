{
    "display_information": {
        "name": "Slack Channel Manager"
    },
    "features": {
        "bot_user": {
            "display_name": "Slack Channel Manager",
            "always_online": false
        }
    },
    "oauth_config": {
        "redirect_urls": [
            "https://slack.com/oauth/callback"
        ],
        "scopes": {
            "bot": [
                "channels:history",
                "channels:read",
                "channels:manage",
                "groups:read",
                "groups:write",
                "groups:history"
            ]
        }
    },
    "settings": {
        "org_deploy_enabled": true,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}